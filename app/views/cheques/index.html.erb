<p id="notice"><%= notice %></p>

<h1>Cheques</h1>

<table class="table">
  <thead>
    <tr>
      <th>Cheque no</th>
      <th>Bank name</th>
      <th>Ch date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cheques.each do |cheque| %>
      <tr>
        <td><%= cheque.Cheque_no %></td>
        <td><%= cheque.Bank_name %></td>
        <td><%= cheque.ch_date %></td>
        <td><%= link_to 'Show', cheque %></td>
        <td><%= link_to 'Edit', edit_cheque_path(cheque) %></td>
        <td><%= link_to 'Destroy', cheque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cheque', new_cheque_path %>
